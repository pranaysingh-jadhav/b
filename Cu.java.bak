import java.util.*;
class  Cu
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("enter any number" );
		int num = sc.nextInt();

		int count=0;
		while (num != 0)
		{
			num /= 10;
			count++;
		}
		System.out.println("count of digits is "+count );
	}
	return;
}
